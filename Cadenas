# Estructura-de-Datos
//Maria Jose Reyes Fuentes 3-J LCS
#include<iostream>

int main() {
    int numero[6] = {10, 20, 30, 40, 50, 60};
    std::cout << numero[5] << std::endl;
    
    int n[3] = {3, 4, 5};
    std::cout << n[0] << std::endl;
    std::cout << n[1] << std::endl;
    std::cout << n[2] << std::endl;

    char c[] = {'l', 'u', 'i', 's'};
    std::cout << c[0] << std::endl;
    
    char s[] = "mona lisa";
    std::cout << s[4] << std::endl;

    return 0;
}
